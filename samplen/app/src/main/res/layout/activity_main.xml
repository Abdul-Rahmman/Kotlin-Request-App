<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:foregroundTint="@android:color/transparent">


    <ImageButton
            android:layout_width="175dp"
            android:layout_height="66dp"
            android:id="@+id/EmploeyButton"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.503"
            android:scaleType="fitXY" android:layout_marginTop="96dp"
            app:layout_constraintTop_toBottomOf="@+id/SignButton"/>
    <ImageButton
            android:layout_width="175dp"
            android:layout_height="66dp"
            android:id="@+id/SignButton"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.503"
            android:scaleType="fitXY" app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="216dp"
            android:soundEffectsEnabled="true"/>
    <ImageView
            android:layout_width="243dp"
            android:layout_height="167dp"
            android:id="@+id/imageView" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent"/>
</android.support.constraint.ConstraintLayout>