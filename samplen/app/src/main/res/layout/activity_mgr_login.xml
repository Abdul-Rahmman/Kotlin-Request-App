<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MgrLogin">

    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textEmailAddress"
            android:ems="10"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:id="@+id/Emailtxt"
            app:layout_constraintHorizontal_bias="0.542" android:layout_marginTop="32dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView2"/>
    <EditText
            android:layout_width="221dp"
            android:layout_height="40dp"
            android:inputType="textPassword"
            android:ems="10"
            android:id="@+id/Passtxt"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.549"
            android:layout_marginEnd="8dp"
            android:layout_marginTop="32dp" app:layout_constraintTop_toBottomOf="@+id/imageView3"/>
    <ImageView
            android:layout_width="220dp"
            android:layout_height="61dp" tools:srcCompat="@tools:sample/avatars[6]"
            android:id="@+id/imageView2" android:layout_marginTop="80dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"/>
    <ImageView
            android:layout_width="220dp"
            android:layout_height="61dp" tools:srcCompat="@tools:sample/avatars[6]"
            android:id="@+id/imageView3" android:layout_marginTop="48dp"
            app:layout_constraintTop_toBottomOf="@+id/Emailtxt" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"/>
    <ImageButton
            android:layout_width="138dp"
            android:layout_height="72dp" tools:srcCompat="@tools:sample/avatars[0]"
            android:id="@+id/imageButton"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="44dp" app:layout_constraintTop_toBottomOf="@+id/Passtxt"/>
</android.support.constraint.ConstraintLayout>